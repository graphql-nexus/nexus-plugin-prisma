{
  "name": "redo-nexus-prisma",
  "version": "0.11.1-5",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": "Flavian Desverne <desverne@prisma.io>",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "release:preview": "dripip preview",
    "release:stable": "dripip stable",
    "publish:pr": "yarn publish --tag pr.${PR} --no-git-tag-version --new-version \"0.0.0-pr.${PR}.$(git show -s head --format='%h')\"",
    "clean": "rm -rf dist",
    "build": "ts-node scripts/ensure-prisma2-version && yarn -s clean && tsc",
    "build:doc": "doctoc README.md && prettier --write README.md",
    "dev": "tsc --watch",
    "dev:test": "ts-node scripts/ensure-prisma2-version && ts-node tests/__ensure-engine.ts && jest --watch",
    "prepublishOnly": "yarn -s build",
    "style": "prettier --write 'src/**/*.ts' 'test/**/*.ts' 'example/**/*.ts'",
    "test": "ts-node scripts/ensure-prisma2-version && ts-node tests/__ensure-engine.ts && jest"
  },
  "dependencies": {
    "camelcase": "^5.3.1",
    "fs-extra": "^8.1.0",
    "pluralize": "^8.0.0",
    "@re-do/utils": "0.0.5"
  },
  "peerDependencies": {
    "@prisma/client": "2.0.0-preview023",
    "graphql": "^14.5.0",
    "nexus": "^0.12.0-rc.13"
  },
  "devDependencies": {
    "@prisma/cli": "^0.1.47",
    "@prisma/client": "2.0.0-preview023",
    "@prisma/fetch-engine": "^0.3.60",
    "@prisma/get-platform": "^0.1.29",
    "@prisma/lift": "^0.3.384",
    "@prisma/sdk": "0.0.222",
    "@types/fs-extra": "^8.0.0",
    "@types/jest": "^25.1.1",
    "@types/node": "^13.7.7",
    "@types/node-fetch": "^2.5.5",
    "@types/pluralize": "^0.0.29",
    "@types/prettier": "^1.19.0",
    "@types/rimraf": "^2.0.3",
    "@types/tmp": "^0.1.0",
    "doctoc": "^1.4.0",
    "dripip": "0.8.0",
    "get-port": "^5.1.1",
    "graphql": "^14.5.4",
    "graphql-request": "^1.8.2",
    "graphql-yoga": "^1.18.3",
    "jest": "^25.1.0",
    "jest-watch-typeahead": "^0.4.0",
    "nexus": "^0.12.0-rc.13",
    "node-fetch": "^2.6.0",
    "prettier": "^1.17.1",
    "prisma2": "2.0.0-preview023",
    "react": "^16.13.0",
    "rimraf": "^3.0.1",
    "strip-ansi": "^6.0.0",
    "tmp": "^0.1.0",
    "ts-jest": "^25.1.0",
    "ts-morph": "^7.0.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.8.3"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
