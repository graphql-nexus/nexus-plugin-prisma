{
  "name": "@morgothulhu/nexus-plugin-prisma",
  "version": "0.40.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": "Flavian Desverne <desverne@prisma.io>",
  "files": [
    "dist",
    "src",
    "scripts",
    "schema.js",
    "schema.d.ts",
    "typegen.js",
    "typegen.d.ts"
  ],
  "scripts": {
    "release": "npm config -L project set @morgothulhu:registry=https://npm.pkg.github.com/ && yarn publish && npm config -L project delete @morgothulhu:registry && yarn publish",
    "clean": "rm -rf dist",
    "build": "yarn clean && yarn build:deps-sync && yarn build:module-facades && tsc",
    "build:deps-sync": "node scripts/ensure-deps-sync",
    "build:module-facades": "node scripts/build-module-facades",
    "build:ci": "tsc",
    "dev": "yarn -s build:module-facades && tsc --watch",
    "prepublishOnly": "yarn -s build",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts' 'examples/**/*.ts'",
    "postinstall": "node ./scripts/postinstall.js",
    "test": "yarn test:run && yarn test:clean",
    "test:clean": "rm -rf tests/tmp",
    "test:run": "jest --testTimeout=10000 --forceExit",
    "test:run:rebaseSnapshots": "jest --testTimeout=10000 --forceExit --updateSnapshot",
    "test:dev": "jest src tests/ --watch",
    "upgrade-prisma-dev": "yarn add prisma @prisma/client@dev @prisma/generator-helper@dev @prisma/migrate@dev @prisma/sdk@dev"
  },
  "peerDependencies": {
    "@prisma/client": "4.0.0",
    "graphql": "16.5.0",
    "nexus": "1.3.0"
  },
  "dependencies": {
    "camelcase": "^6.3.0",
    "endent": "2.1.0",
    "fs-jetpack": "4.3.1",
    "lodash": "4.17.21",
    "pluralize": "8.0.0",
    "semver": "7.3.7"
  },
  "devDependencies": {
    "@prisma-labs/prettier-config": "0.1.0",
    "@prisma/client": "4.0.0",
    "@prisma/generator-helper": "4.0.0",
    "@prisma/migrate": "4.0.0",
    "@prisma/internals": "4.0.0",
    "@types/jest": "28.1.5",
    "@types/lodash": "4.14.182",
    "@types/node": "^14.18.21",
    "@types/node-fetch": "2.6.2",
    "@types/pluralize": "0.0.29",
    "@types/prettier": "2.6.3",
    "@types/semver": "7.3.10",
    "cash-rm": "0.2.0",
    "execa": "^5.1.1",
    "get-port": "^5.1.1",
    "graphql": "16.5.0",
    "graphql-request": "4.3.0",
    "graphql-scalars": "1.17.0",
    "graphql-yoga": "2.13.1",
    "jest": "28.1.2",
    "jest-watch-typeahead": "2.0.0",
    "nexus": "1.3.0",
    "node-fetch": "3.2.8",
    "prettier": "2.7.1",
    "prisma": "4.0.0",
    "strip-ansi": "^6.0.1",
    "ts-jest": "28.0.5",
    "ts-morph": "15.1.0",
    "ts-node": "10.8.2",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "prettier": "@prisma-labs/prettier-config"
}
