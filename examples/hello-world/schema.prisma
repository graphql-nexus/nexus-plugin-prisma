datasource db {
  provider = "sqlite"
  url      = "sqlite:./dev.db"
}

generator photonjs {
  provider = "prisma-client-js"
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  birthDate DateTime
  posts     Post[]
}

model Post {
  id     String @id @default(cuid())
  author User[]
}
