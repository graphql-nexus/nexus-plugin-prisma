{
  "name": "nexus-plugin-prisma",
  "version": "0.0.0-see-git-tags",
  "main": "dist/framework-entrypoint.js",
  "types": "dist/framework-entrypoint.d.ts",
  "license": "MIT",
  "author": "Flavian Desverne <desverne@prisma.io>",
  "files": [
    "dist",
    "src",
    "scripts",
    "typegen.js",
    "typegen.d.ts",
    "schema.js",
    "schema.d.ts",
    "client.js",
    "client.d.ts"
  ],
  "scripts": {
    "release:preview": "dripip preview",
    "release:stable": "dripip stable",
    "release:pr": "dripip pr",
    "clean": "rm -rf dist",
    "build": "yarn clean && yarn build:module-facades && tsc && ts-node scripts/post-build",
    "build:module-facades": "node scripts/build-module-facades",
    "build:ci": "tsc",
    "dev": "yarn -s build:module-facades && tsc --watch",
    "prepublishOnly": "yarn -s build",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts' 'examples/**/*.ts'",
    "postinstall": "node ./scripts/postinstall.js",
    "test": "ts-node tests/__ensure-engine && jest --forceExit",
    "test:dev": "ts-node tests/__ensure-engine && jest src tests/schema --watch",
    "test:unit": "jest src --testTimeout=15000 --forceExit",
    "test:schema": "yarn test --testTimeout=15000 tests/schema",
    "test:fw:e2e:mysql": "yarn test --testTimeout=400000 tests/framework/e2e/mysql.test.ts",
    "test:fw:e2e:postgres": "yarn test --testTimeout=400000 tests/framework/e2e/postgres.test.ts",
    "test:fw:e2e:sqlite": "yarn test --testTimeout=400000 tests/framework/e2e/sqlite.test.ts",
    "test:db:setup": "docker-compose down && rm -rf .mysql-data && rm -rf .postgres-data && docker-compose up -d"
  },
  "dependencies": {
    "@prisma/cli": "2.1.3",
    "@prisma/client": "2.1.3",
    "@prisma/sdk": "2.1.3",
    "camelcase": "^6.0.0",
    "chalk": "^4.0.0",
    "common-tags": "^1.8.0",
    "dotenv": "^8.2.0",
    "fs-extra": "^9.0.0",
    "fs-jetpack": "^2.4.0",
    "outdent": "^0.7.1",
    "pluralize": "^8.0.0",
    "prismjs": "^1.20.0",
    "stacktrace-parser": "^0.1.10"
  },
  "devDependencies": {
    "@nexus/schema": "0.14.0",
    "@prisma-labs/prettier-config": "0.1.0",
    "@prisma/fetch-engine": "2.1.3",
    "@prisma/get-platform": "2.1.3",
    "@prisma/migrate": "2.1.3",
    "@types/common-tags": "1.8.0",
    "@types/fs-extra": "8.1.0",
    "@types/jest": "25.1.4",
    "@types/node": "13.9.5",
    "@types/node-fetch": "2.5.5",
    "@types/pluralize": "0.0.29",
    "@types/prettier": "1.19.1",
    "@types/prismjs": "1.16.1",
    "@types/rimraf": "3.0.0",
    "@types/tmp": "0.1.0",
    "dripip": "0.9.0",
    "get-port": "5.1.1",
    "graphql": "^14.6.0",
    "graphql-request": "1.8.2",
    "graphql-scalars": "1.1.5",
    "graphql-yoga": "1.18.3",
    "jest": "25.2.4",
    "jest-watch-typeahead": "0.6.0",
    "nexus": "0.24.0",
    "node-fetch": "2.6.0",
    "node-pty": "0.9.0",
    "prettier": "2.0.2",
    "pretty-quick": "2.0.1",
    "react": "16.13.0",
    "rimraf": "3.0.2",
    "rxjs": "6.5.5",
    "strip-ansi": "6.0.0",
    "tmp": "0.1.0",
    "ts-jest": "25.3.0",
    "ts-morph": "7.1.0",
    "ts-node": "8.10.1",
    "tslib": "2.0.0",
    "typescript": "3.9.5"
  },
  "prisma": {
    "version": "de2bc1cbdb5561ad73d2f08463fa2eec48993f56"
  },
  "prettier": "@prisma-labs/prettier-config"
}
