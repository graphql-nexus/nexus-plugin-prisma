{
  "name": "nexus-plugin-prisma",
  "version": "0.0.0-see-git-tags",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": "Flavian Desverne <desverne@prisma.io>",
  "files": [
    "dist",
    "src",
    "scripts",
    "schema.js",
    "schema.d.ts",
    "typegen.js",
    "typegen.d.ts"
  ],
  "scripts": {
    "release:preview": "dripip preview",
    "release:stable": "dripip stable",
    "release:pr": "dripip pr",
    "clean": "rm -rf dist",
    "build": "yarn clean && yarn build:deps-sync && yarn build:module-facades && tsc",
    "build:deps-sync": "node scripts/ensure-deps-sync",
    "build:module-facades": "node scripts/build-module-facades",
    "build:ci": "tsc",
    "dev": "yarn -s build:module-facades && tsc --watch",
    "prepublishOnly": "yarn -s build",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts' 'examples/**/*.ts'",
    "postinstall": "node ./scripts/postinstall.js",
    "test": "ts-node tests/__ensure-engine && jest --testTimeout=10000 --forceExit",
    "test:dev": "ts-node tests/__ensure-engine && jest src tests/ --watch"
  },
  "peerDependencies": {
    "@nexus/schema": "^0.15.0",
    "@prisma/client": "2.8.1",
    "graphql": "14.6.0"
  },
  "dependencies": {
    "camelcase": "^6.0.0",
    "fs-jetpack": "^2.4.0",
    "lodash": "^4.17.19",
    "outdent": "^0.7.1",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@nexus/schema": "^0.15.0",
    "@prisma-labs/prettier-config": "0.1.0",
    "@prisma/cli": "2.7.0",
    "@prisma/client": "2.7.0",
    "@prisma/fetch-engine": "2.7.0",
    "@prisma/get-platform": "2.7.0",
    "@prisma/migrate": "2.7.0",
    "@types/jest": "25.2.3",
    "@types/lodash": "4.14.157",
    "@types/node": "13.13.12",
    "@types/node-fetch": "2.5.7",
    "@types/pluralize": "0.0.29",
    "@types/prettier": "1.19.1",
    "@types/rimraf": "3.0.0",
    "dripip": "0.9.0",
    "get-port": "5.1.1",
    "graphql": "14.6.0",
    "graphql-request": "1.8.2",
    "graphql-scalars": "1.2.1",
    "graphql-yoga": "1.18.3",
    "jest": "25.5.4",
    "jest-watch-typeahead": "0.6.0",
    "nexus": "0.26.0",
    "node-fetch": "2.6.0",
    "prettier": "2.0.5",
    "react": "16.13.1",
    "rimraf": "3.0.2",
    "rxjs": "6.5.5",
    "strip-ansi": "6.0.0",
    "ts-jest": "25.5.1",
    "ts-morph": "7.1.2",
    "ts-node": "8.10.2",
    "tslib": "2.0.0",
    "typescript": "3.9.5"
  },
  "prisma": {
    "version": "de2bc1cbdb5561ad73d2f08463fa2eec48993f56"
  },
