{
  "name": "example",
  "private": true,
  "version": "0.0.0",
  "main": "dist/main.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn -s clean && yarn -s generate && tsc",
    "style": "prettier --write 'src/**/*'",
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/main.ts",
    "generate": "yarn -s generate:prisma && yarn -s generate:nexus",
    "generate:prisma": "prisma2 generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "data:inject": "ts-node prisma/fake-data.ts"
  },
  "dependencies": {
    "graphql": "^14.3.1",
    "graphql-yoga": "^1.18.2",
    "nexus": "^0.12.0-beta.12",
    "nexus-prisma": "^0.5.0-next.5"
  },
  "devDependencies": {
    "@types/faker": "^4.1.5",
    "@types/ws": "^6.0.3",
    "faker": "^4.1.0",
    "prettier": "^1.18.2",
    "prisma2": "^2.0.0-alpha.185",
    "ts-node": "^8.3.0",
    "ts-node-dev": "^1.0.0-pre.41",
    "typescript": "^3.5.3"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
