{
  "name": "nexus-plugin-prisma",
  "version": "0.0.0-see-git-tags",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": "Flavian Desverne <desverne@prisma.io>",
  "files": [
    "dist",
    "src",
    "scripts",
    "schema.js",
    "schema.d.ts",
    "typegen.js",
    "typegen.d.ts"
  ],
  "scripts": {
    "release:preview": "dripip preview",
    "release:stable": "dripip stable",
    "release:pr": "dripip pr",
    "clean": "rm -rf dist",
    "build": "yarn clean && yarn build:deps-sync && yarn build:module-facades && tsc",
    "build:deps-sync": "node scripts/ensure-deps-sync",
    "build:module-facades": "node scripts/build-module-facades",
    "build:ci": "tsc",
    "dev": "yarn -s build:module-facades && tsc --watch",
    "prepublishOnly": "yarn -s build",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts' 'examples/**/*.ts'",
    "postinstall": "node ./scripts/postinstall.js",
    "test": "jest --testTimeout=10000 --forceExit",
    "test:dev": "jest src tests/ --watch",
    "upgrade-prisma-dev": "yarn add @prisma/cli@dev @prisma/client@dev @prisma/fetch-engine@dev @prisma/get-platform@dev @prisma/migrate@dev @prisma/sdk@dev"
  },
  "peerDependencies": {
    "@nexus/schema": "^0.13.0 || ^0.14.0 || ^0.15.0 || ^0.16.0 || ^0.17.0 || ^0.18.0",
    "@prisma/client": "2.10.x || 2.11.x",
    "graphql": "^15.3.0"
  },
  "dependencies": {
    "camelcase": "^6.0.0",
    "fs-jetpack": "^4.0.0",
    "lodash": "^4.17.20",
    "outdent": "^0.7.1",
    "pluralize": "^8.0.0",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "@nexus/schema": "0.18.0",
    "@prisma-labs/prettier-config": "0.1.0",
    "@prisma/cli": "^2.12.0-dev.49",
    "@prisma/client": "^2.12.0-dev.49",
    "@prisma/fetch-engine": "^2.12.0-dev.49",
    "@prisma/get-platform": "^2.12.0-dev.49",
    "@prisma/migrate": "^2.12.0-dev.49",
    "@prisma/sdk": "^2.12.0-dev.49",
    "@types/jest": "26.0.15",
    "@types/lodash": "4.14.165",
    "@types/node": "13.13.32",
    "@types/node-fetch": "2.5.7",
    "@types/pluralize": "0.0.29",
    "@types/prettier": "2.1.5",
    "@types/rimraf": "3.0.0",
    "@types/semver": "7.3.4",
    "dripip": "0.10.0",
    "get-port": "5.1.1",
    "graphql": "15.4.0",
    "graphql-request": "3.3.0",
    "graphql-scalars": "1.5.0",
    "graphql-yoga": "1.18.3",
    "jest": "26.6.3",
    "jest-watch-typeahead": "0.6.1",
    "node-fetch": "2.6.1",
    "prettier": "2.2.0",
    "rimraf": "3.0.2",
    "strip-ansi": "6.0.0",
    "ts-jest": "26.4.4",
    "ts-morph": "8.2.0",
    "ts-node": "9.0.0",
    "tslib": "2.0.3",
    "typescript": "4.1.2"
  },
  "prettier": "@prisma-labs/prettier-config"
}
