{
  "name": "nexus-prisma",
  "version": "0.0.0-see-git-tags",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": "Flavian Desverne <desverne@prisma.io>",
  "files": [
    "dist",
    "src",
    "typegen.js",
    "typegen.d.ts"
  ],
  "scripts": {
    "release:preview": "dripip preview",
    "release:stable": "dripip stable",
    "release:pr": "dripip pr",
    "clean": "rm -rf dist",
    "build": "ts-node scripts/ensure-prisma2-version && yarn -s clean && tsc && ts-node scripts/post-build",
    "dev": "tsc --watch",
    "dev:test": "ts-node scripts/ensure-prisma2-version && ts-node tests/__ensure-engine && jest --watch",
    "prepublishOnly": "yarn -s build",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts' 'examples/**/*.ts'",
    "test": "ts-node scripts/ensure-prisma2-version && ts-node tests/__ensure-engine && yarn build && jest --forceExit"
  },
  "dependencies": {
    "camelcase": "^6.0.0",
    "fs-extra": "^9.0.0",
    "outdent": "^0.7.1",
    "pluralize": "^8.0.0"
  },
  "peerDependencies": {
    "@nexus/schema": "^0.13.1 || ^0.14.0",
    "@prisma/client": "2.0.0",
    "graphql": "^14.5.0"
  },
  "devDependencies": {
    "@nexus/schema": "0.13.1",
    "@prisma-labs/prettier-config": "^0.1.0",
    "@prisma/cli": "2.0.0",
    "@prisma/client": "2.0.0",
    "@prisma/fetch-engine": "2.0.0",
    "@prisma/get-platform": "2.0.0",
    "@prisma/migrate": "2.0.0",
    "@prisma/sdk": "2.0.0",
    "@types/fs-extra": "8.1.0",
    "@types/jest": "25.1.4",
    "@types/node": "13.9.5",
    "@types/node-fetch": "2.5.5",
    "@types/pluralize": "0.0.29",
    "@types/prettier": "1.19.1",
    "@types/rimraf": "3.0.0",
    "@types/tmp": "0.1.0",
    "dripip": "0.9.0",
    "get-port": "5.1.1",
    "graphql": "14.6.0",
    "graphql-request": "1.8.2",
    "graphql-scalars": "^1.1.5",
    "graphql-yoga": "1.18.3",
    "jest": "25.2.4",
    "jest-watch-typeahead": "0.4.2",
    "node-fetch": "2.6.0",
    "prettier": "2.0.2",
    "react": "16.13.0",
    "rimraf": "3.0.2",
    "strip-ansi": "6.0.0",
    "tmp": "0.1.0",
    "ts-jest": "25.3.0",
    "ts-morph": "7.1.0",
    "ts-node": "8.10.1",
    "typescript": "3.9.3"
  },
  "prettier": "@prisma-labs/prettier-config"
}
