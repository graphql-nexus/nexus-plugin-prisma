{
  "name": "@morgothulhu/nexus-plugin-prisma",
  "version": "0.43.7",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": "Flavian Desverne <desverne@prisma.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/morgothulhu/nexus-plugin-prisma.git"
  },
  "contributors": [
    {
      "email": "francois@rleyh.com",
      "name": "Morgothulhu",
      "url": "https://github.com/morgothulhu"
    }
  ],
  "keywords": [
    "prisma",
    "nexus"
  ],
  "files": [
    "dist",
    "src",
    "scripts",
    "schema.js",
    "schema.d.ts",
    "typegen.js",
    "typegen.d.ts"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "update:all": "npm-check -i \"{camelcase,escape-string-regexp}\" --update-all --exact --skip-unused .",
    "build": "yarn clean && yarn build:deps-sync && yarn build:module-facades && tsc",
    "build:deps-sync": "node scripts/ensure-deps-sync",
    "build:module-facades": "node scripts/build-module-facades",
    "build:ci": "tsc",
    "dev": "yarn -s build:module-facades && tsc --watch",
    "prepublishOnly": "yarn -s build",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts' 'examples/**/*.ts'",
    "postinstall": "node ./scripts/postinstall.js",
    "test": "yarn test:run && yarn test:clean",
    "test:clean": "rm -rf tests/tmp",
    "test:run": "jest --forceExit",
    "test:run:rebaseSnapshots": "jest --forceExit --updateSnapshot",
    "test:dev": "jest src tests/ --watch",
    "upgrade-prisma-dev": "yarn add prisma @prisma/client@dev @prisma/generator-helper@dev @prisma/migrate@dev @prisma/sdk@dev"
  },
  "peerDependencies": {
    "@prisma/client": "4.7.1",
    "graphql": "16.6.0",
    "nexus": "1.3.0"
  },
  "dependencies": {
    "@prisma/internals": "4.7.1",
    "camelcase": "^6.3.0",
    "endent": "2.1.0",
    "escape-string-regexp": "^4.0.0",
    "fs-jetpack": "5.1.0",
    "lodash": "4.17.21",
    "pluralize": "8.0.0",
    "semver": "7.3.8"
  },
  "devDependencies": {
    "@prisma-labs/prettier-config": "0.1.0",
    "@prisma/client": "4.7.1",
    "@prisma/generator-helper": "4.7.1",
    "@prisma/migrate": "4.7.1",
    "@types/jest": "29.2.3",
    "@types/lodash": "4.14.191",
    "@types/node": "^18.11.10",
    "@types/node-fetch": "2.6.2",
    "@types/pluralize": "0.0.29",
    "@types/prettier": "2.7.1",
    "@types/semver": "7.3.13",
    "cash-rm": "0.2.0",
    "execa": "^5.1.1",
    "get-port": "^5.1.1",
    "graphql": "16.6.0",
    "graphql-request": "5.0.0",
    "graphql-scalars": "1.20.1",
    "graphql-yoga": "3.1.1",
    "jest": "29.3.1",
    "jest-watch-typeahead": "2.2.1",
    "nexus": "1.3.0",
    "npm-check": "6.0.1",
    "node-fetch": "3.3.0",
    "prettier": "2.8.0",
    "prisma": "4.7.1",
    "strip-ansi": "^6.0.1",
    "ts-jest": "29.0.3",
    "ts-morph": "17.0.1",
    "ts-node": "10.9.1",
    "tslib": "2.4.1",
    "typescript": "4.9.3"
  },
  "prettier": "@prisma-labs/prettier-config"
}
