{
  "name": "@morgothulhu/nexus-plugin-prisma",
  "version": "0.40.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": "Flavian Desverne <desverne@prisma.io>",
  "files": [
    "dist",
    "src",
    "scripts",
    "schema.js",
    "schema.d.ts",
    "typegen.js",
    "typegen.d.ts"
  ],
  "scripts": {
    "release": "npm config -L project set @morgothulhu:registry=https://npm.pkg.github.com/ && yarn publish && npm config -L project delete @morgothulhu:registry && yarn publish",
    "clean": "rm -rf dist",
    "build": "yarn clean && yarn build:deps-sync && yarn build:module-facades && tsc",
    "build:deps-sync": "node scripts/ensure-deps-sync",
    "build:module-facades": "node scripts/build-module-facades",
    "build:ci": "tsc",
    "dev": "yarn -s build:module-facades && tsc --watch",
    "prepublishOnly": "yarn -s build",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts' 'examples/**/*.ts'",
    "postinstall": "node ./scripts/postinstall.js",
    "test": "yarn test:run && yarn test:clean",
    "test:clean": "rm -rf tests/tmp",
    "test:run": "jest --testTimeout=10000 --forceExit --maxWorkers=1",
    "test:run:rebaseSnapshots": "jest --testTimeout=10000 --forceExit --maxWorkers=1 --updateSnapshot",
    "test:dev": "jest src tests/ --watch",
    "upgrade-prisma-dev": "yarn add prisma @prisma/client@dev @prisma/generator-helper@dev @prisma/migrate@dev @prisma/sdk@dev"
  },
  "peerDependencies": {
    "@prisma/client": "3.6.x",
    "graphql": "^15.3.0",
    "nexus": "^1.0.0"
  },
  "dependencies": {
    "camelcase": "^6.2.0",
    "endent": "^2.1.0",
    "fs-jetpack": "^4.1.0",
    "lodash": "^4.17.20",
    "pluralize": "^8.0.0",
    "semver": "^7.3.4"
  },
  "devDependencies": {
    "@prisma-labs/prettier-config": "0.1.0",
    "@prisma/client": "3.6.0",
    "@prisma/generator-helper": "3.6.0",
    "@prisma/migrate": "3.6.0",
    "@prisma/sdk": "3.6.0",
    "@types/jest": "26.0.20",
    "@types/lodash": "4.14.167",
    "@types/node": "14.14.20",
    "@types/node-fetch": "2.5.7",
    "@types/pluralize": "0.0.29",
    "@types/prettier": "2.1.6",
    "@types/semver": "7.3.4",
    "cash-rm": "0.2.0",
    "execa": "^5.0.0",
    "get-port": "5.1.1",
    "graphql": "15.4.0",
    "graphql-request": "3.4.0",
    "graphql-scalars": "1.7.0",
    "graphql-yoga": "1.18.3",
    "jest": "26.6.3",
    "jest-watch-typeahead": "0.6.1",
    "nexus": "^1.0.0",
    "node-fetch": "2.6.1",
    "prettier": "2.2.1",
    "prisma": "3.6.0",
    "strip-ansi": "6.0.0",
    "ts-jest": "26.4.4",
    "ts-morph": "9.1.0",
    "ts-node": "9.1.1",
    "tslib": "2.1.0",
    "typescript": "4.1.3"
  },
  "prettier": "@prisma-labs/prettier-config"
}
