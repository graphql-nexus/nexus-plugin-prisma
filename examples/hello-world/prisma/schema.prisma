datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator prisma_client {
  provider = "prisma-client-js"
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  birthDate DateTime
  posts     Post[]
}

model Post {
  id       String @id @default(cuid())
  authors   User[] @relation(references: [id])
}