  "scripts": {
    "release:preview": "dripip preview",
    "release:stable": "dripip stable",
    "release:pr": "dripip pr",
    "clean": "rm -rf dist",
    "build": "ts-node scripts/ensure-prisma2-version && yarn -s clean && tsc",
    "build:doc": "doctoc README.md && prettier --write README.md",
    "dev": "tsc --watch",
    "dev:test": "ts-node scripts/ensure-prisma2-version && ts-node tests/__ensure-engine && jest --watch",
    "prepublishOnly": "yarn -s build",
    "style": "prettier --write 'src/**/*.ts' 'test/**/*.ts' 'example/**/*.ts'",
    "test": "ts-node scripts/ensure-prisma2-version && ts-node tests/__ensure-engine && jest"
  },
  "dependencies": {
    "camelcase": "^5.3.1",
    "fs-extra": "^8.1.0",
    "pluralize": "^8.0.0"
  },
  "peerDependencies": {
    "@nexus/schema": "^0.12.0-rc.13",
    "@prisma/client": "2.0.0-preview025",
    "graphql": "^14.5.0"
  },
  "devDependencies": {
    "@nexus/schema": "^0.12.0-rc.13",
    "@prisma/cli": "2.0.0-preview025",
    "@prisma/client": "2.0.0-preview025",
